    "notifications",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "32": "icons/gdc-32.png",
    "48": "icons/gdc-48.png",
    "64": "icons/gdc-64.png",
    "128": "icons/gdc-128.png"
  }
}
