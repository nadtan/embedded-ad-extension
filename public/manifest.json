{
  "short_name": "GDC PostMessage",
  "name": "GDC PostMessage",
  "version":"1.0",
  "manifest_version": 2,
  "description": "Communicate with AD by postMessage",
  "devtools_page": "index.html",
  "content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk='; object-src 'self'",
  "permissions": [
    "activeTab",
    "declarativeContent",
